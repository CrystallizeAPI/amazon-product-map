{
  "name": "amazon-product-map",
  "version": "0.0.0",
  "description": "Maps your Crystallize Products in the Amazon Marketplace",
  "main": "index.js",
  "scripts": {
    "preversion": "yarn run lint && yarn run test",
    "lint": "standard",
    "create": "claudia create --region eu-central-1 --no-optional-dependencies --api-module index"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "standard": {
    "ignore": ["dist/"],
    "globals": ["describe", "it", "expect"]
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "pino-pretty": "^3.5.0",
    "prettier-standard": "^16.1.0",
    "standard": "^14.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrystallizeAPI/amazon-product-map.git"
  },
  "keywords": ["AWS", "Amazon", "Crystallize"],
  "author": "Stavros Sotiropoulos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CrystallizeAPI/amazon-product-map/issues"
  },
  "homepage": "https://github.com/CrystallizeAPI/amazon-product-map#readme",
  "dependencies": {
    "claudia-api-builder": "^4.1.2"
  }
}
